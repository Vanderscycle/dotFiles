{
  "schema": [
    "Build",
    "Test"
  ],
  "build": [
    {
      "cmd": "apk",
      "args": [
        "upgrade --available && sync"
      ]
    },
    {
      "cmd": "apk",
      "args": [
        "kubectl docker"
      ]
    }
  ],
  "test": [
    {
      "cmd": "kubectl",
      "args": [
        "version --client"
      ]
    },
    {
      "cmd": "docker",
      "args": [
        "--version"
      ]
    }
  ]
}
